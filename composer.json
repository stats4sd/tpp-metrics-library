{
    "name": "stats4sd/laravel-template",
    "type": "project",
    "description": "A Laravel Data Platform Template.",
    "keywords": ["framework", "laravel", "template"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "awcodes/filament-badgeable-column": "^2.3",
        "awcodes/filament-table-repeater": "^3.0",
        "backpack/crud": "^6.0",
        "backpack/permissionmanager": "^7.0",
        "backpack/theme-tabler": "^1.2",
        "filament/filament": "^3.0",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^10.0",
        "laravel/telescope": "^4.17",
        "laravel/tinker": "^2.5",
        "maatwebsite/excel": "^3.1",
        "orangehill/iseed": "^3.0",
        "phpsa/filament-authentication": "^3.0",
        "pusher/pusher-php-server": "^7.2",
        "pxlrbt/filament-environment-indicator": "^2.0",
        "symfony/http-client": "^6.2",
        "symfony/mailgun-mailer": "^6.2"
    },
    "require-dev": {
        "backpack/generators": "^4.0",
        "barryvdh/laravel-ide-helper": "^2.10",
        "doctrine/dbal": "^3.5",
        "fakerphp/faker": "^1.9.1",
        "filament/upgrade": "3.2",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan filament:upgrade"
        ],
        "post-install-cmd": ["php artisan storage:link --quiet"]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
